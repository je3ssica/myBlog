<!DOCTYPE html>
<html>
  <head>
    <title>MyBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <nav>
          <ul>
              <li>Hem</li>
              <li>Om mig</li>
              <li>Portfolio</li>
          </ul>
      </nav>
      <% if flash[:notice] %>
      <div class="notice">
          <%= flash[:notice]%>
      </div>
      <% end %>
    <%= yield %>
  </body>
</html>
